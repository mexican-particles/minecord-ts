{
  "name": "minecord-ts",
  "version": "0.0.26",
  "description": "Connects Discord and Minecraft Servers without any mods or plugins.",
  "main": "./lib/index.js",
  "module": "src/index.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "minecord": "./dist/index.js"
  },
  "engines": {
    "node": "^16.12"
  },
  "scripts": {
    "dist": "rm -rf ./dist && yarn build && cp -a ./src/config.example.json ./dist/config.json && cp -a ./entrypoint/* ./dist",
    "build": "babel src --extensions '.ts' --out-dir dist",
    "start": "nodemon dist/index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mexican-particles/minecord-ts.git"
  },
  "keywords": [
    "Discord",
    "Minecraft"
  ],
  "author": "moriyama@node-link.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mexican-particles/minecord-ts"
  },
  "homepage": "https://github.com/mexican-particles/minecord-ts#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/chokidar": "^2.1.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.4",
    "babel-minify": "^0.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-minify": "^0.5.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "chokidar": "^3.5.2",
    "commander": "^8.3.0",
    "discord.js": "^13.2.0",
    "iconv-lite": "^0.6.3",
    "nodemon": "^2.0.14",
    "rcon-ts": "^1.2.3"
  },
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "tsConfig": "tsconfig.json"
    }
  },
  "testMatch": [
    "/tests/**/*.test.ts"
  ]
}
